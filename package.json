    "dotenv": "17.2.3",
    "drizzle-orm": "1.0.0-beta.2-f9236e3",
    "graphql-auto-query": "1.0.4",
    "hono": "4.11.0",
    "jose": "6.1.3",
    "pg": "8.16.3",
    "pothos-drizzle-generator": "0.0.2"
  },
  "devDependencies": {
    "@eslint/js": "9.39.2",
